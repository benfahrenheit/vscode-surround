{
    "name": "vscode-surround",
    "displayName": "Surround",
    "description": "A port of tpope's vim-surround to vscode",
    "version": "0.0.1",
    "publisher": "benfahrenheit",
    "icon": "assets/icon.png",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/benfahrenheit/vscode-surround"
    },
    "engines": {
        "vscode": "^0.10.10"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.swapQuotes"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.swapQuotes",
                "title": "Swap single/double quotes"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}